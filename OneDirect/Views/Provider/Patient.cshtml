                        $("input:radio[id='extension']").prop("checked",false);
                        $("input:radio[id='flexion']").prop("checked",true);
                        $("#romextension").hide();
                        $("#romflexion").show();
                        $("#compextension").hide();
                        $("#compflexion").show();
