    <script type="text/javascript">
        "use strict";
        var charRegex = /\D+/g;
        var idleTime = 0;
        var clientTimeout =@Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(ConfigVars.NewInstance.clienttimeout))

          function activitylogclick() {
              $("#divpopup").css("display", "block");
              $("#mask").css("display", "block");
            };

         var interval = null;
        $(document).ready(function () {
            if ($("#reviewtimer").length > 0) {
                var lstorage = "date" + "@Context.Session.GetString("ReviewID")";
                var d = localStorage.getItem(lstorage);
                if (!d) {
                    d = new Date();
                    d.setHours(0);
                    d.setMinutes(0);
                    d.setSeconds(0, 0);
                    localStorage.setItem(lstorage, d);
                }
                else {

                    d = new Date(d);
                }
                interval = setInterval(function () {
                   
                    document.getElementById("m").innerHTML = ("0" + d.getMinutes()).slice(-2);
                    document.getElementById("s").innerHTML = ("0" + d.getSeconds()).slice(-2);
                    d.setTime(d.getTime() + 1000);
                    localStorage.setItem(lstorage, d);
                }, 1000);
               
            }
        });
        $(document).ready(function () {
           


            $(window).on('beforeunload', function (e) {
                
                showMask();
               
            });
            $(window).on('unload', function () {
                
            });

           

            var idleInterval = window.setInterval(timerIncrement, 60000);
            $(this).mousemove(function (e) {
                idleTime = 0;
            });
            $(this).keypress(function (e) {
                idleTime = 0;
            });
            function timerIncrement() {
                idleTime = idleTime + 1;
                
                if (idleTime > parseInt(clientTimeout)) { // 20 minutes
                    window.location = $("#redirecturl").attr("href");
                }
            }

            var pathName = window.location.pathname.match(charRegex);
            if (pathName.toString().split('/').length == 2) {
                pathName = pathName + "/";

            }
            var currentLink;
            if (pathName != "/") {
                currentLink = $("ul.sidebar-menu a[href*='" + pathName + "']");
            }
            else {
                if ("@Context.Session.GetString("UserType")" != "2") {
                    currentLink = $("ul.sidebar-menu a[href='/Therapist/Index']");
                }
                if ("@Context.Session.GetString("UserType")" == "2") {
                    currentLink = $("ul.sidebar-menu a[href='/Therapist/Profile']");
                }
            }

            if (currentLink) {
                if (currentLink.find("i").hasClass('text-aqua'))
                    currentLink.find("i").removeClass("text-aqua");
                currentLink.find("i").addClass("text-yellow");
            }

        });

        function showMask(isDelay) {

            if (isDelay) {
                console.log("if mask");
                setTimeout(function () {
                    document.getElementById("divMask").style.display = "block";
                   
                }, 10);
            }
            else {
                console.log("else mask");
                setTimeout(function () {
                    document.getElementById("divMask").style.display = "block";
                   
                }, 10);
            }
        }

        function hideMask() {
            document.getElementById("divMask").style.display = "none";
        }


        function btnSaveClick() {
            btnCancelClick();
            var log = {
                ActivityType: "Review",
                Duration: $('#txtduration').val(),
                Comment: $('#txtcomment').val(),
                RecordType: $("#recordType").val()
            }
            console.log(log);
            $.ajax({
                url: '@Url.Action("insertuseractivity", "useractivitylog")',
                data: JSON.stringify(log),
                type: 'post',
                dataType: 'json',
                contentType: "application/json; charset=utf-8",
                success: function (data) {
                    if (data != null) {
                        console.log(data);
                        if (data.result == "success") {
                            
                            btnCancelClick();
                            window.location.reload();
                        }
                    }
                },
                error: function (data) {
                    
                }
            });
        }
        function btnCancelClick() {
            document.getElementById("divpopup").style.display = "none";
            document.getElementById("mask").style.display = "none";
        }
        var pcontroller = '@this.ViewContext.RouteData.Values["controller"].ToString()';
        console.log(pcontroller);




        function btnUpdateClick() {
            showMask();
            btnCancelClick();
            var log = {
                assessmentComment: $('#txtcomment').val(),
            }
            console.log(log);
            $.ajax({
                url: '@Url.Action("updatereviewactivity", "review")',
                data: JSON.stringify(log),
                type: 'post',
                dataType: 'json',
                contentType: "application/json; charset=utf-8",
                success: function (data) {
                    if (data != null) {
                        console.log(data);
                        if (data.result == "success") {
                            
                            localStorage.clear();
                            
                            btnCancelClick();
                            if ($("#hdnlogout").val() == "logout") {

                                window.location.href="@Url.Action("Signout", "Login", new { id = "Clean Logout" })";
                            }
                            else {
                               

                                window.location.href = $("#returnurl").val();
                            }

                        }
                    }
                },
                error: function (data) {
                    showMask();
                }
            });
        }


        history.pushState(pcontroller, null, location.href);
        window.onpopstate = function () {
            
            if ($("#activityid").val() != "") {
                if (pcontroller == "Review") {
                    if (confirm('Exit Review mode?')) {
                        document.getElementById("divpopup").style.display = "block";
                        document.getElementById("mask").style.display = "block";
                    }

                    history.pushState(null, null, document.URL);
                }
            }
            else {
                history.go(-1);
            }
        };

        $(document).ready(function () {
            $("#logout").on("click",function (event) {
                if ($("#activityid").val() != "") {
                    if (confirm('Exit Review mode?')) {
                        document.getElementById("divpopup").style.display = "block";
                        document.getElementById("mask").style.display = "block";
                        $("#hdnlogout").val("logout");
                    }
                    event.preventDefault();
                    
                }
            })

            $("ul.sidebar-menu li a").on("click", function (event) {
                if ($("#activityid").val() != "") {
                    $("#returnurl").val($(this).attr("href"));
                    if (confirm('Exit Review mode?')) {
                        document.getElementById("divpopup").style.display = "block";
                        document.getElementById("mask").style.display = "block";
                    }
                    event.preventDefault();
                   
                }
            });

            $("ol.breadcrumb li a").on("click", function (event) {
                if ($("#activityid").val() != "") {
                    $("#returnurl").val($(this).attr("href"));
                    if (confirm('Exit Review mode?')) {
                        document.getElementById("divpopup").style.display = "block";
                        document.getElementById("mask").style.display = "block";
                    }
                    event.preventDefault();

                }
            });
        });



    </script>

    @Html.Raw(TempData["msg"])

</body>
</html>